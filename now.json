{
  "version": 2,
  "builds": [{ "src": "server/*.js", "use": "@now/node" }],
  "routes": [
    { "src": "/api/get-widgets", "dest": "server/getWidgets.js" },
    {
      "src": "/api/create-widget",
      "dest": "server/createWidget.js"
    },
    {
      "src": "/api/get-widget/",
      "dest": "server/getWidget.js"
    }
  ]
}
