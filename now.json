{
  "version": 2,
  "builds": [{ "src": "serverless/*.js", "use": "@now/node" }],
  "routes": [
    { "src": "/api/get-widgets", "dest": "serverless/getWidgets.js" },
    {
      "src": "/api/create-widget",
      "dest": "serverless/createWidget.js"
    },
    {
      "src": "/api/update-widget",
      "dest": "serverless/updateWidget.js"
    },
    {
      "src": "/api/get-widget/",
      "dest": "serverless/getWidget.js"
    }
  ]
}
